  <name>Signatures</name>
  <description>Provides configurable signatures per contact to be used in e-mails and letters, for single and mass mailings.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jens Schuppe</author>
    <email>schuppe@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.signatures</url>
    <url desc="Documentation">https://docs.civicrm.org/signatures/en/latest/</url>
    <url desc="Support">https://github.com/systopia/de.systopia.signatures/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>1.2-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Signatures</namespace>
  </civix>
</extension>
